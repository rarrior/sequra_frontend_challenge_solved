{
  "name": "@sequra/installment-widget",
  "version": "1.0.0",
  "description": "SeQura installment payment widget for e-commerce sites",
  "main": "dist/sequra-widget.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "start:api": "cd api && npm start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve:dist": "npx serve dist -p 3000",
    "demo:prod": "npm run build && concurrently --kill-others --names \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run start:api\" \"npm run serve:dist\""
  },
  "keywords": [
    "sequra",
    "payment",
    "installments",
    "widget",
    "e-commerce"
  ],
  "author": "Raul Arriola Gomez",
  "license": "MIT",
  "dependencies": {
    "lucide-react": "^0.553.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.19",
    "ts-jest": "^29.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/react-webpack5": "^10.0.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.35",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^10.0.7",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.12.1",
    "msw-storybook-addon": "^2.0.6",
    "storybook": "^10.0.7",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
